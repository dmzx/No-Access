{
	"name": "dmzx/noaccess",
	"type": "phpbb-extension",
	"description": "This extension will show message to members that don't have access to a forum.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2017-04-30",
	"keywords": [
		"phpbb",
		"extension",
		"noaccess"
	],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		},
		{
			"name": "martin",
			"homepage": "http://www.martins.homelinux.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "No Access",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.3"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "noaccess_version.json"
		}
	}
}