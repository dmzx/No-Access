{
	"name": "dmzx/noaccess",
	"type": "phpbb-extension",
	"description": "This extension will show message to members that don't have access to a forum.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.1",
	"time": "2020-01-13",
	"keywords": [
		"phpbb",
		"extension",
		"noaccess"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		},
		{
			"name": "martin",
			"homepage": "https://www.martins-phpbb.com",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "No Access",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<4.4.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "noaccess_version.json",
			"ssl": true
		}
	}
}
